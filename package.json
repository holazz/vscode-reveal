{
  "name": "vsc-reveal",
  "displayName": "Reveal",
  "version": "0.0.3",
  "publisher": "holazz",
  "packageManager": "pnpm@6.32.3",
  "description": "Select opened file at explorer",
  "keywords": [
    "reveal",
    "file",
    "explorer",
    "sidebar"
  ],
  "homepage": "https://github.com/holazz/vscode-reveal#readme",
  "bugs": {
    "url": "https://github.com/holazz/vscode-reveal/issues"
  },
  "license": "MIT",
  "author": "zz",
  "repository": {
    "type": "git",
    "url": "https://github.com/holazz/vscode-reveal"
  },
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:reveal.active"
  ],
  "icon": "res/icon.png",
  "contributes": {
    "commands": [
      {
        "command": "reveal.active",
        "title": "%reveal.title%",
        "icon": {
          "dark": "./res/reveal-dark.svg",
          "light": "./res/reveal-light.svg"
        }
      }
    ],
    "keybindings": [
      {
        "when": "editorIsOpen",
        "command": "reveal.active",
        "key": "ctrl+alt+u",
        "mac": "cmd+alt+u"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "reveal.active",
          "when": "view == workbench.explorer.fileView && editorIsOpen",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies --allow-star-activation",
    "pack": "vsce package --no-dependencies --allow-star-activation",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp --commit --push --tag && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.18.8",
    "@antfu/ni": "^0.13.2",
    "@types/node": "^17.0.21",
    "@types/vscode": "^1.65.0",
    "bumpp": "^7.1.1",
    "eslint": "^8.11.0",
    "pnpm": "^6.32.3",
    "rimraf": "^3.0.2",
    "tsup": "^5.12.1",
    "typescript": "^4.6.2",
    "vitest": "^0.7.4",
    "vsce": "^2.7.0"
  }
}
